<div class="modal fade" id="DeleteStorageModel" tabindex="-1" role="dialog" aria-labelledby="DeleteStorageModel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Are u sure that u want to delete this Storage?</h4>
            </div>
            <div class="modal-body">
                <h4>ID:<span data-bind="text: storageViewModel.selectedStorage.Id"></span></h4>
                <h4>NAME:<span data-bind="text: storageViewModel.selectedStorage.Name"></span></h4>
                <h4>NUMBER:<span data-bind="text: storageViewModel.selectedStorage.Number"></span></h4>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" data-bind="click: storageViewModel.RemoveStorage">Delete</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="EditStorageModel" tabindex="-1" role="dialog" aria-labelledby="EditStorageModel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Create Form</h4>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Id</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" placeholder="Id" data-bind="value: storageViewModel.selectedStorage.Id" readonly="true">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputPassword3" class="col-sm-2 control-label">Number</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" placeholder="Number" data-bind="value: storageViewModel.selectedStorage.Number">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">Name</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" placeholder="Book Name" data-bind="value: storageViewModel.selectedStorage.Name">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" data-bind="click: storageViewModel.UpdateStorage">Update book</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="CreateStorageModel" tabindex="-1" role="dialog" aria-labelledby="CreateStorageModel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Edit Form</h4>
            </div>
            <div class="modal-body">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"> Room Number</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  data-bind="value: storageViewModel.selectedStorage.Number">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Room Name</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" data-bind="value: storageViewModel.selectedStorage.Name">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" data-bind="click: storageViewModel.CreateStorage">Create Storage</button>
            </div>
        </div>
    </div>
</div>